{
    "name": "venoudev/results",
    "keywords": [
      "venoudev",
      "results",
      "api",
      "json",
      "laravel",
      "lumen",
      "response"
    ],
    "description": "An awesome package for construct a very understandable Json structure response in your Laravel or Lumen project for your API Rest.",
    "authors": [
        {
            "name": "philipretl",
            "email": "philipretl@gmail.com"
        }
    ],
    "require": {
       "php": "^7.4|^8.0",
       "illuminate/http": "^7.0|^8.0|^9.0",
       "illuminate/support": "^7.0|^8.0|^9.0",
       "illuminate/console": "^7.0|^8.0|^9.0"
   },
   "require-dev": {
       "orchestra/testbench": "^5.0|^6.0",
       "phpunit/phpunit": "^9.0"
   },
    "autoload": {
        "psr-4": {
            "Venoudev\\Results\\": "src/"
        },
        "classmap": [
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Venoudev\\Results\\Providers\\ResultsServiceProvider"
            ],
            "aliases": {
                "ResultManager": "Venoudev\\Results\\Facades\\ResultManagerFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "license": "MIT"
